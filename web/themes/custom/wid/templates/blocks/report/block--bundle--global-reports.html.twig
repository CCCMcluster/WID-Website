{% set classes = [
  'block',
  'block-' ~ configuration.provider|clean_class,
  'block-' ~ plugin_id|clean_class
] %}
{% set contentClasses = [
  'content'
] %}
<div class="global-report-wrapper">
  <div {{ attributes.addClass(classes) }}>
      <div class="container">
        <div class="global-report-container">
          {{ title_prefix }}
          {% if label %}
            <h2{{ title_attributes }}>{{ label }}</h2>
          {% endif %}
          {{ title_suffix }}
          {% block content %}
          <div{{ content_attributes.addClass(contentClasses) }}>
            <div>{{ content.body }} <a href="{{ path('page_manager.page_view_global_reports_global_reports-panels_variant-0') }}"><p class="global-report-button">More on Global Reports Page</p></a></div>
          </div>
        </div>
      </div>
      {% endblock %}
  </div>
</div>
